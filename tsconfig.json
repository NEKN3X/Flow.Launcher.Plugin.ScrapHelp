/* https://aka.ms/tsconfig */
{
	"compilerOptions": {
		"target": "es2016",
		"baseUrl": "src",
		"module": "node16",
		"moduleResolution": "node16",
		"paths": {},
		"types": [
			"vitest/globals"
		],
		"strict": true,
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true
	},
	"include": [
		"src/**/*.ts"
	]
}
